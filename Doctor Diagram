@startuml
left to right direction
actor "Doctor" as fc
rectangle MisterED{
  usecase "Log in/Log out" as UC1
  usecase "Patients" as UC2
  usecase "View Schedule" as UC3
  usecase "Pronouns" as UC4
  usecase "Name" as UC5
  usecase "Issue" as UC6
  usecase "Allergies" as UC7
}
fc --> UC1
fc --> UC2
fc --> UC3
UC2 .> UC4: extend
UC2 .> UC5: extend
UC2 .> UC6: extend
UC2 .> UC7: extend

@enduml
